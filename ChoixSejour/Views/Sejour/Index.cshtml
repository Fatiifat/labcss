@model List<ChoixSejour.Models.Sejour>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>

    <table>
        <tr>
            <th>Lieu</th>
            <th>Téléphone</th>
        </tr>
        @foreach(var sejour in Model){
        <tr>
            <td>@sejour.Lieu</td>
            <td>@sejour.Telephone</td>
        </tr>
        }
    </table>

</body>
</html>
